<h2 class="page-header">Problem list</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<div>
  <div class="form-group">
      {{#each problems}}
        <div class="alert alert-danger">
            {{code}} 
            <br>
            {{description}}
            <br>
            {{points}} 
        </div>
        
      {{/each}}
  </div>

  </div>
  

</div>

